local player = game.Players.LocalPlayer
local playerGui = player:WaitForChild("PlayerGui")

-- ScreenGui oluÅŸtur
local screenGui = Instance.new("ScreenGui")
screenGui.Parent = playerGui

-- Ana Frame
local mainFrame = Instance.new("Frame")
mainFrame.Size = UDim2.new(0, 300, 0, 500) -- Daha bÃ¼yÃ¼k boyut
mainFrame.Position = UDim2.new(0.5, -150, 0.5, -150)
mainFrame.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
mainFrame.BorderSizePixel = 2
mainFrame.Active = true
mainFrame.Draggable = true
mainFrame.Parent = screenGui

-- BaÅŸlÄ±k
local title = Instance.new("TextLabel")
title.Size = UDim2.new(1, 0, 0, 30)
title.Position = UDim2.new(0, 0, 0, 0)
title.Text = "TA SCRIPTS V7 ðŸ‡¹ðŸ‡·"
title.TextSize = 18
title.TextColor3 = Color3.new(1, 1, 1)
title.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
title.Font = Enum.Font.SourceSansBold
title.Parent = mainFrame

-- Buton oluÅŸturma fonksiyonu
local function createButton(name, posY, callback)
    local button = Instance.new("TextButton")
    button.Size = UDim2.new(1, 0, 0, 25)
    button.Position = UDim2.new(0, 0, 0, posY)
    button.Text = name
    button.TextSize = 16
    button.Font = Enum.Font.SourceSans
    button.BackgroundColor3 = Color3.fromRGB(70, 70, 70)
    button.TextColor3 = Color3.new(1, 1, 1)
    button.Parent = mainFrame

    button.MouseButton1Click:Connect(callback)

    return button
end

-- 1. NPC ÃœSTÃœ GÄ°YME
createButton("NPC ÃœSTÃœ GÄ°YME", 40, function()
    local character = player.Character
    if character then
        for _, obj in pairs(character:GetChildren()) do
            if obj:IsA("Accessory") or obj:IsA("Shirt") or obj:IsA("Pants") then
                obj:Destroy()
            end
        end
        local newShirt = Instance.new("Shirt", character)
        newShirt.ShirtTemplate = "http://www.roblox.com/asset/?id=5023835792"

        local newPants = Instance.new("Pants", character)
        newPants.PantsTemplate = "http://www.roblox.com/asset/?id=5023848330"
    end
end)

-- 2. Ä°SYANCI OLMA
createButton("Ä°SYANCI OLMA", 70, function()
    local teams = game:GetService("Teams")
    if teams:FindFirstChild("Ä°syancÄ±lar") then
        player.Team = teams["Ä°syancÄ±lar"]
        local backpack = player:FindFirstChild("Backpack")
        if backpack then
            local akm = teams["Ä°syancÄ±lar"]:FindFirstChild("AKM")
            if akm then
                local newTool = akm:Clone()
                newTool.Parent = backpack
            end
        end
    end
end)

