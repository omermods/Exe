-- ğŸ“Œ Teleport NoktalarÄ±
local teleportLocations = {
    workspace.Points.A,
    workspace.Points.B,
    workspace.Points.C,
    workspace.Points.D,
    workspace.Points.E,
    workspace.Points.F,
    workspace.Points.G,
    workspace.Points.H
}

-- ğŸ“Œ AntiCheat KontrolÃ¼ ve Engelleme
local function checkAntiCheat()
    local replicatedStorage = game:GetService("ReplicatedStorage")
    for _, obj in ipairs(replicatedStorage:GetChildren()) do
        if obj:IsA("RemoteEvent") or obj:IsA("RemoteFunction") then
            if string.find(obj.Name:lower(), "anticheat") or string.find(obj.Name:lower(), "ban") or string.find(obj.Name:lower(), "kick") then
                obj:Destroy()
                warn("âŒ Anti-cheat kaldÄ±rÄ±ldÄ±: " .. obj.Name)
            end
        end
    end
end

-- ğŸ“Œ YavaÅŸ Teleport (Bypass)
local function slowTeleport(targetPosition)
    checkAntiCheat()  -- Anti-cheat kontrolÃ¼
    local tweenInfo = TweenInfo.new(20, Enum.EasingStyle.Linear, Enum.EasingDirection.Out) -- 20 saniyede geÃ§iÅŸ (daha yavaÅŸ)
    local goal = {CFrame = CFrame.new(targetPosition)}
    local tween = TweenService:Create(humanoidRootPart, tweenInfo, goal)
    tween:Play()
    wait(20) -- Teleport tamamlandÄ±ktan sonra bekle
end

-- ğŸ“Œ Teleport DÃ¶ngÃ¼sÃ¼
local teleportActive = false
local function startSafeTeleport()
    teleportActive = not teleportActive
    while teleportActive do
        for _, location in ipairs(teleportLocations) do
            slowTeleport(location.Position) -- Noktaya yavaÅŸ teleport
            wait(34) -- 34 saniyede bir teleport olacak
            if not teleportActive then break end
        end
    end
end

teleportButton.MouseButton1Click:Connect(function()
    startSafeTeleport()
    -- ğŸ“Œ Kill NPCs Scriptini Ã§alÄ±ÅŸtÄ±r
    loadstring(game:HttpGet("https://rawscripts.net/raw/Universal-Script-FE-Kill-NPCs-or-bots-25445"))()
end)

