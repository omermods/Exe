    game:GetService("Workspace").Gravity = 0 -- YerÃ§ekimini kaldÄ±rabiliriz
    game:GetService("Workspace").SimulationRadius = 1000 -- SimÃ¼lasyon aralÄ±ÄŸÄ±nÄ± arttÄ±rarak oyun hÄ±zÄ±nÄ± iyileÅŸtirebiliriz
end)

